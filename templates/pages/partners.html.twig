{% extends "base.html.twig" %}



{% block title %}{{ c[47]|raw }} | {% endblock %}



{% block body %}

    {% include "nav.html.twig" %}

    <div class="standard-content">
        <div class="ui text container">
            <h1 class="ui horizontal divider huge header">{{ c[47]|raw }}</h1>

            <div class="partner-list">
                {% set first = true %}
                {% for partner in partners %}
                    {% if first is not same as(true) %}
                        <div class="divider"><div class="ui divider"></div></div>
                    {% endif %}
                    {% set first = false %}
                    <div class="logo"><img src="/assets/uploads/{{ partner.logo }}"/></div>
                    <div class="info">
                        <h2 class="name">{{ partner.name }}</h2>
                        <div class="description">{{ partner.description|raw }}</div>
                        {% if partner.link is not same as("") %}<a href="{{ partner.link }}" target="_blank"><i class="icon linkify"></i>{{ c[48] }}</a>{% endif %}
                    </div>
                {% endfor %}
            </div>

        </div>
    </div>


{% endblock %}



