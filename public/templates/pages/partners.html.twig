{% extends "base.html.twig" %}



{% block title %}{{ c[47]|raw }} | {% endblock %}



{% block body %}

    {% include "nav.html.twig" %}

    <div class="standard-content">
        <div class="ui text container">
            <h1 class="ui horizontal divider huge header">{{ c[47]|raw }}</h1>

            <div class="partner-list">
                {% for partner in partners %}
                        {% if partner.link is same as("") %}<div class="partner-logo"
                        {% else %}<a class="partner-logo" href="{{partner.link}}" target="_blank"{% endif %}  data-inverted="" data-tooltip="{{partner.name}}" data-position="top center">
                        <img src="/assets/uploads/{{partner.logo}}"/>
                        {% if partner.link is same as("") %}</div>
                        {% else %}</a>{% endif %}
                {% endfor %}
            </div>

        </div>
    </div>


{% endblock %}



